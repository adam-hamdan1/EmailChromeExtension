{
    "manifest_version": 3,
    "name": "Chrome Email Sorter",
    "version": "1.0",
    "permissions": [
      "storage",
      "identity",
      "identity.email",
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "oauth2": {
      "client_id": "YOUR_CLIENT_ID",
      "scopes": [
        "https://mail.google.com/"
      ]
    }
  }
  